@using FFRaidAnalytics.Models;
@model IEnumerable<EncounterModel>

    <div class="content">
        <h1 class="content-title">Encounters</h1>

        <table class="table">
            <thead>
                <tr>
                    <th>Encounter</th>
                </tr>
            </thead>
            <tbody>
                @foreach (EncounterModel encounter in Model)
                {
                    <tr>
                        <td><a asp-action="Details" asp-route-id="@encounter.Id">@Html.DisplayFor(item => encounter.EncounterName)</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>